    function formatDate(date) {
        if(typeof date !== 'undefined' && date !== null) {
            var dateFormatted = ('0' + date.getDate()).slice(-2) + '/'+ ('0' + (date.getMonth()+1)).slice(-2) + '/' + date.getFullYear();
            return dateFormatted;
        }
    }

